{
    "$schema": "https://json.schemastore.org/staticwebapp.config.json",
    "platform": {
        "apiRuntime": "node:18"
    },
    "globalHeaders": {
        "Access-Control-Allow-Origin": "*"
    },
    "routes": [{
        "route": "/*",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
    }]
}